<hazelcast xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.hazelcast.com/schema/config
                               http://www.hazelcast.com/schema/config/hazelcast-config-3.5.xsd"
           xmlns="http://www.hazelcast.com/schema/config">
    <network>
        <join>
            <multicast enabled="true"/>
        </join>
    </network>
    <native-memory enabled="true" allocator-type="POOLED">
        <size value="100" unit="GIGABYTES"/>
    </native-memory>
    <map name="historicalTransactions">
        <in-memory-format>NATIVE</in-memory-format>
        <backup-count>0</backup-count>
        <async-backup-count>1</async-backup-count>
    </map>

    <license-key>
        HazelcastEnterprise#9999Nodes#9999Clients#HDMemory:99999999GB#w7yAkRj1IbHcBfVimEOuKr638599939999peZ319999z05999Wn149zGxG09
    </license-key>
</hazelcast>
